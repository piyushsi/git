{
  "manifest_version": 2,
  "name": "Github Solutions",
  "description": "Get your arranged issue page",
  "version": "0.1",
  "author": "Piyush Sinha",
  "content_scripts": [
    {
      "matches": ["https://github.com/*/*/issues"],
      "js": ["src/loader.js"],
      "css": ["src/style.css"]
    }
  ],
  "permissions": ["tabs"]
}
